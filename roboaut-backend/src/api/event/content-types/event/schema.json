{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "text",
      "required": true
    },
    "Type": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Workshop",
        "Seminars",
        "Competitons",
        "Other"
      ]
    },
    "Date": {
      "type": "date",
      "required": true
    },
    "Thumbnail": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
